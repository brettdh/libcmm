<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="edu.umich.intnw.scout"
      android:versionCode="1"
      android:versionName="1.0">
    <permission xmlns:android="http://schemas.android.com/apk/res/android"
                android:name="edu.umich.intnw.scout.SERVICE_PERMISSION" 
                android:label="ConnScoutService permission" 
                android:protectionLevel="normal" />
    <application android:label="@string/app_name" android:debuggable="true">
        <activity android:name=".ConnScout"
                  android:label="@string/app_name"
                  android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <service android:name=".ConnScoutService" 
                 android:permission="edu.umich.intnw.scout.SERVICE_PERMISSION" />
        
    </application>
    
    <uses-permission 
      android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="edu.umich.intnw.scout.SERVICE_PERMISSION" />
    
    <uses-sdk android:minSdkVersion="3" />
</manifest> 
